{
    "printWidth": 120,
    "proseWrap": "preserve",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.md",
            "options": {
                "proseWrap": "preserve",
                "printWidth": 120,
                "tabWidth": 2
            }
        }
    ]
}